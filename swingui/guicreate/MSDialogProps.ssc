  propPath <- "MSProcess.prp"
  infoPath <- "MSProcess.fni"  


  ## Create a counter for spaces within dialogs
  currentSpaces <- list(numberOfSpaces = 0)

  createSpaces <- function(currentSpaces, n) {
    i <- currentSpaces[["numberOfSpaces"]]
    spaceNames <- paste("MSSpace", (i + 1):(i + n), sep = "")
    for(j in 1:n) {
      guiCreate("Property",
                Name = spaceNames[j],
                Type = "Normal",
                DialogControl = "New Line")
    }
    return(list(numberOfSpaces = i + n, spaceNames = spaceNames))
  }

